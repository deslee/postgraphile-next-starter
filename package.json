    "@types/recompose": "^0.30.6",
    "@types/styled-jsx": "^2.2.8",
    "@types/uuid": "^3.4.4",
    "@types/yup": "^0.26.13",
    "cross-env": "^5.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "renamer": "^1.1.1",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.4.5"
  }
}
